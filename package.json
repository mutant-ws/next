{
  "name": "@mutantlove/next",
  "version": "0.0.1",
  "description": "Official tracking library for the Mutant board",
  "homepage": "https://github.com/mutantlove/next",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutantlove/next.git"
  },
  "bugs": {
    "url": "https://github.com/mutantlove/next/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm",
    "web": "https://mutant.love"
  },
  "main": "dist/index.js",
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update --save-exact",
    "----BUNDLE": "",
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "----TEST": "",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint src",
    "lint:md": "markdownlint *.md",
    "test": "riteway -r @babel/register src/*.test.js | tap-nirvana",
    "tdd": "nodemon -e js --watch src --exec \"npm test\""
  },
  "dependencies": {
    "@mutantlove/m": "0.31.0",
    "cuid": "2.1.6",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@asd14/eslint-config": "5.2.0",
    "@babel/cli": "7.6.2",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-pipeline-operator": "7.5.0",
    "@babel/preset-env": "7.6.2",
    "@babel/register": "7.6.2",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.6",
    "eslint": "6.5.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-unicorn": "12.0.1",
    "markdownlint-cli": "0.18.0",
    "npm-check": "5.9.0",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "riteway": "6.1.0",
    "semantic-release": "15.13.24",
    "tap-nirvana": "1.1.0"
  }
}
