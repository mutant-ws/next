{
  "name": "@mutant-ws/next",
  "version": "0.0.1",
  "description": "Official browser tracking library for the Mutant Workspace.",
  "homepage": "https://github.com/mutant-ws/next",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutant-ws/next.git"
  },
  "bugs": {
    "url": "https://github.com/mutant-ws/next/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andrei-dumitrescu"
  },
  "main": "dist/index.js",
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update",
    "----BUNDLE": "",
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "----TEST": "",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint src",
    "lint:md": "markdownlint *.md",
    "test": "riteway -r @babel/register src/*.test.js | tap-nirvana",
    "tdd": "nodemon -e js --watch src --exec \"npm test\""
  },
  "dependencies": {
    "@mutant-ws/m": "^1.10.0",
    "cuid": "^2.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@mutant-ws/eslint-config": "^5.9.0",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^17.2.0",
    "markdownlint-cli": "^0.22.0",
    "npm-check": "^5.9.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "riteway": "^6.1.1",
    "semantic-release": "^17.0.4",
    "tap-nirvana": "^1.1.0"
  }
}
