{
  "parser": "babel-eslint",
  "root": true,
  "extends": [ "@mutantlove/eslint-config/targets/node" ],
  "settings": {
    // Recommended if you use eslint_d
    "import/cache": {
      "lifetime": 5,
    }
  },
  "rules": {
    "promise/avoid-new": "off",
  },
  "env": {
    "browser": true,
  }
}
